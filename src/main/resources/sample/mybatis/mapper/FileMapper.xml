<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="SAMPLE_FILE_MAPPER">

	<insert id="addNewFile" useGeneratedKeys="true" keyProperty="id" parameterType="sample.jetty.domain.FileBean">
	insert into test.TB_FILE (CREATE_DT,CREATED_BY,LAST_UPD_DT,LAST_UPDED_BY,CREATE_TIME,RELATE_PATH,FILE_TYPE)
	values (#{createDt}, #{createdBy}, #{lastUpdDt}, #{lastUpdedBy}, #{createTime}, #{relatePath}, #{fileType})
	</insert>
	
</mapper>